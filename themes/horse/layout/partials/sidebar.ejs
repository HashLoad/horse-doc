<div class="sidebar">
  <div class="sidebar-inner">
    <ul class="main-menu">
      <%- partial('partials/main_menu', { context: 'sidebar' }) %>
    </ul>
    <div class="list">
      <% if (type !== 'search') { %>
        <h2>
          <% titles = {
            tutorials: 'Tutoriais',
            examples: 'Exemplos',
            guide: 'Guia',
          } %>
          <%- titles[type] %>
        </h2>
        <%- partial('partials/toc', { type: type }) %>
      <% } %>
    </div>
  </div>
</div>